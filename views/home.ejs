<%-include('partials/header')%>

<div class="postDetail">
    <h1>Home</h1>
    <p><%= startingContent %></p>
    
    <%contents.forEach((post) => { %>
    <div id="homePost">
        <h1><%= post.title %></h1>
        <p><%= post.content.slice(0, 100) %>...</p>
        <a href="/posts/<%= post._id %>">Read more</a>
    </div>
    <% }); %>
</div>

<%-include('partials/footer')%>
